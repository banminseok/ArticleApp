@model List<string>

@if (Model != null && Model.Count > 0)
{
    @foreach (var m in Model)
    {
        <div class="post_item">
            <div class="post_item_text">
                <span class="">@m</span>
            </div>
        </div>
    }
}
else
{
    <div class="text-center">
        항목이 없습니다.
    </div>
}